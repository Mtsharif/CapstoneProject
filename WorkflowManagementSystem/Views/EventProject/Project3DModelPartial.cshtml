@model WorkflowManagementSystem.ViewModels.Project3DModelViewModel

@{
    Layout = null;
}

@using (Html.BeginForm("Project3DModelPartial", "EventProject", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        <div class="form-group">
            @Html.LabelFor(model => model.ThreeDModelFile, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBox("ThreeDModelFile", "", new { type = "file" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Submit" class="btn btn-default" id="Create3DModelButton" />
            </div>
        </div>
    </div>
}

